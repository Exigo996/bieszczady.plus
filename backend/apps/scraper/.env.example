# Facebook Scraper Configuration
# ================================

# Browser settings
SCRAPER_HEADLESS=true

# File paths
SCRAPER_COOKIE_FILE=data/cookies/facebook_cookies.json
SCRAPER_OUTPUT_DIR=data/scraped_events
SCRAPER_LOG_LEVEL=INFO

# Scraping limits
SCRAPER_MAX_POSTS=50

# Django integration
SCRAPER_DJANGO_IMPORT=true
SCRAPER_AUTO_TRANSLATE=true

# Facebook credentials (optional - can use CLI instead)
# FACEBOOK_EMAIL=your@email.com
# FACEBOOK_PASSWORD=yourpassword
